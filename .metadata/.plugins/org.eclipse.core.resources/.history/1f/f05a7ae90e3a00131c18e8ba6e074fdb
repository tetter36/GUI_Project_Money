						textToAmt.setText(conversionString);
					}
					else if(chosenRateTo.equalsIgnoreCase("CNY")){
						conversion = (EURrate/CNYrate) * Float.parseFloat(textFromAmt.getText());
						conversionString = Float.toString(conversion);
						textToAmt.setText(conversionString);
					}
