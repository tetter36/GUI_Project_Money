					else if(chosenRateTo.equalsIgnoreCase("CHF")){
						conversion = (KRWrate/CHFrate) * Float.parseFloat(textFromAmt.getText());
						conversionString = Float.toString(conversion);
						textToAmt.setText(conversionString);
